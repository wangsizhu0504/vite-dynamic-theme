{
  "name": "vite-dynamic-theme",
  "version": "0.1.5",
  "description": "Vite plugin for dynamically changing the theme color of the interface",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "es",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.client.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm && tsc -p tsconfig.client.json",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build",
    "release:prepare": "shipjs prepare",
    "prepublishOnly": "pinst --disable && npm run build",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "vite"
  ],
  "author": "Vben",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangsizhu0504/vite-dynamic-theme"
  },
  "homepage": "https://github.com/wangsizhu0504/vite-dynamic-theme/tree/master/#readme",
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/less": "^3.0.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vue/compiler-sfc": "^3.2.45",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.5",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "shipjs": "^0.24.4",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@types/node": "^18.11.9",
    "@types/tinycolor2": "^1.4.3",
    "clean-css": "^5.3.1",
    "debug": "^4.3.4",
    "esbuild": "^0.15.13",
    "esbuild-plugin-alias": "^0.2.1",
    "tinycolor2": "^1.4.2"
  }
}
